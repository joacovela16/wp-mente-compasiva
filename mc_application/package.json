{
  "name": "mentecompasiva-theme",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prod": "windicss './**/*.{php,svelte}' -d -to assets/styles/windi.css",
    "pwatch": "windicss  './**/*.{php,scss}' './theme.css' '../mc_plugin/**/*.php' -b -d -to assets/styles/windi.css --config windi.config.js ",
    "watch": "tailwindcss  -i ./theme.scss -o assets/styles/windi.css --watch "
  },
  "devDependencies": {
    "daisyui": "1.24.3",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5",
    "tailwindcss": "3.0.15",
    "windicss": "3.4.3",
    "tailwindcss-textshadow": "2.1.3"
  }
}
